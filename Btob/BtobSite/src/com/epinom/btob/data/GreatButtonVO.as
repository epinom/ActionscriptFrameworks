package com.epinom.btob.data
{
	public class GreatButtonVO
	{
			private var _id:String;
			private var _textColor:String;
			private var _htmlText:Boolean
			private var _text:String;
			
			public function GreatButtonVO() {}
			
			public function get id():String { return _id; }
			public function set id(value:String):void {_id = value; }				
			
			public function get textColor():String { return _textColor; }
			public function set textColor(value:String):void { _textColor = value; }
			
			public function get htmlText():Boolean { return _htmlText; }
			public function set htmlText(value:Boolean):void { _htmlText = value; }
			
			public function get text():String { return _text; }
			public function set text(value:String):void { _text = value; }
	}
}