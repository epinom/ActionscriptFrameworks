package com.epinom.btob.data
{
	public class ClientVO
	{
		private var _id:uint;
		private var _ref:String;
		private var _name:String;
		private var _urlLogo:String;
		private var _urlClient:String;
		private var _title:String;
		private var _urlImage:String;
		private var _socialList:Array;
		private var _caseList:Array;
		
		public function ClientVO() 
		{
			_socialList = new Array();
			_caseList = new Array(); 
		}
		
		public function get id():uint { return _id; }
		public function set id(value:uint):void { _id = value; }
		
		public function get ref():String { return _ref; }
		public function set ref(value:String):void { _ref = value; }
		
		public function get name():String { return _name; }
		public function set name(value:String):void { _name = value; }
		
		public function get urlLogo():String { return _urlLogo; }
		public function set urlLogo(value:String):void { _urlLogo = value; }
		
		public function get urlClient():String { return _urlClient; }
		public function set urlClient(value:String):void { _urlClient = value; }
		
		public function get title():String { return _title; }
		public function set title(value:String):void { _title = value; }

		public function get urlImage():String { return _urlImage; }
		public function set urlImage(value:String):void { _urlImage = value; }
		
		public function get socialList():Array { return _socialList; }
		public function set socialList(value:Array):void { _socialList = value; }
		
		public function get caseList():Array { return _caseList; }
		public function set caseList(value:Array):void { _caseList = value; }
		
		public function toString():String
		{
			var str:String = "[ClientVO] : " + "id=" + _id + " ref=" + _ref  + " name=" + _name;
			return str;
		}
	}
}