package com.epinom.btob.data
{
	public class SharingVO
	{
		private var _youtubeVideoURL:String;
		private var _facebookItemList:Array;
		private var _blogItemList:Array;
		private var _twitterItemList:Array;
		
		public function SharingVO() 
		{
			_facebookItemList = new Array();
			_blogItemList = new Array();
			_twitterItemList = new Array();
		}
		
		public function get youtubeVideoURL():String { return _youtubeVideoURL; }
		public function set youtubeVideoURL(value:String):void { _youtubeVideoURL = value; }
		
		public function get facebookItemList():Array { return _facebookItemList; }
		public function set facebookItemList(value:Array):void { _facebookItemList = value; } 
		
		public function get blogItemList():Array { return _blogItemList; }
		public function set blogItemList(value:Array):void { _blogItemList = value; } 
		
		public function get twitterItemList():Array { return _twitterItemList; }
		public function set twitterItemList(value:Array):void { _twitterItemList = value; } 
	}
}