package com.epinom.btob.data
{
	public class ItemSocialVO
	{
		private var _id:uint;
		private var _date:String;
		private var _dateTextColor:String;
		private var _dateHTMLText:Boolean;	
		private var _text:String;
		private var _textTextColor:String;
		private var _textHTMLText:Boolean; 	
		private var _url:String;
		
		public function ItemSocialVO() {}
		
		public function get id():uint { return _id; }
		public function set id(value:uint):void {_id = value; }
			
		public function get date():String { return _date; }
		public function set date(value:String):void {_date = value; }
		
		public function get dateTextColor():String { return _dateTextColor; }
		public function set dateTextColor(value:String):void {_dateTextColor = value; }
		
		public function get dateHTMLText():Boolean { return _dateHTMLText; }
		public function set dateHTMLText(value:Boolean):void { _dateHTMLText = value; }
		
		public function get text():String { return _text; }
		public function set text(value:String):void {_text = value; }
		
		public function get textTextColor():String { return _textTextColor; }
		public function set textTextColor(value:String):void {_textTextColor = value; }
		
		public function get textHTMLText():Boolean { return _textHTMLText; }
		public function set textHTMLText(value:Boolean):void { _textHTMLText = value; }
		
		public function get url():String { return _url; }
		public function set url(value:String):void {_url = value; }
	}
}