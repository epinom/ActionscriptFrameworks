package com.epinom.btob.data
{
	public class ServiceVO
	{
		private var _id:String;
		private var _caseList:Array;	// Lista de objetos (Object), cada objeto contiene dos campos enteros (clientID, caseID)
		
		public function ServiceVO(id:String)
		{
			_id = id;
			_caseList = new Array();
		}
		
		public function get id():String { return _id; }
		public function set id(value:String):void { _id = value; }
		
		public function get caseList():Array { return _caseList; }
		public function set caseList(value:Array):void { _caseList = value; }
	}
}